<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>KeepAlive</key>
		<false/>
		<key>Label</key>
		<string>com.devteam.BootNeuter</string>
		<key>LaunchOnlyOnce</key>
		<true/>
		<key>ProgramArguments</key>
		<array>
			<string>/Applications/BootNeuter.app/BootNeuter</string>
			<string>-autoMode</string>
			<string>YES</string>
			<string>-RegisterForSystemEvents</string>
			<string>YES</string>
			<string>-unlockBaseband</string>
			<string>YES</string>
			<string>-selfDestruct</string>
			<string>YES</string>
		</array>
		<key>RunAtLoad</key>
		<true/>
		<key>StandardErrorPath</key>
		<string>/var/logs/bn-stderr</string>
		<key>StandardOutPath</key>
		<string>/var/logs/bn-stdout</string>
	</dict>
</plist>
